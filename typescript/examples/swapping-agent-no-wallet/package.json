{
  "name": "swapping-agent-no-wallet",
  "version": "1.0.0",
  "description": "Ember On-chain Actions swapping Agent Example without integrated wallet",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc --build && cpx \"encyclopedia/**/*\" dist/encyclopedia",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "pnpm --filter arbitrum-vibekit build && pnpm --filter ember-api build && pnpm --filter test-utils build && NODE_OPTIONS=\"--no-warnings\" tsx node_modules/mocha/bin/mocha --require ./test/setup-env.ts --exit --timeout 120000 --config ../../.mocharc.json 'test/**/*.test.ts'",
    "inspect:npx": "pnpm run build && npx -y @modelcontextprotocol/inspector node ./dist/index.js",
    "docker:build": "docker build -t swapping-agent-no-wallet ../../",
    "docker:run": "docker run --env-file .env -p 3001:3001 swapping-agent-no-wallet",
    "docker:compose:up": "docker compose up --build --no-log-prefix",
    "docker:compose:down": "docker compose down",
    "docker:compose:build": "docker compose build --no-cache",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\""
  },
  "dependencies": {
    "@google-a2a/types": "workspace:*",
    "@modelcontextprotocol/sdk": "catalog:",
    "@openzeppelin/contracts": "^5.3.0",
    "ai": "catalog:",
    "arbitrum-vibekit-core": "workspace:*",
    "cors": "^2.8.5",
    "dotenv": "catalog:",
    "ember-api": "workspace:*",
    "ethers": "^5.7.2",
    "express": "catalog:",
    "fastify": "^5.1.0",
    "viem": "^2.19.8",
    "ws": "^8.18.2",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@eslint/js": "catalog:",
    "@types/chai": "catalog:",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/mocha": "catalog:",
    "@types/node": "catalog:",
    "chai": "catalog:",
    "cpx": "^1.5.0",
    "eslint": "catalog:",
    "globals": "^16.1.0",
    "mocha": "catalog:",
    "mocha-suppress-logs": "catalog:",
    "prettier": "catalog:",
    "test-utils": "workspace:*",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:"
  }
}
